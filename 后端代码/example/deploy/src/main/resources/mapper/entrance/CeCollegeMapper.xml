<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mock.example.modules.entrance.mapper.CeCollegeMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.mock.example.modules.entrance.entity.model.CeCollege">
        <id column="id" property="id" />
        <result column="college_no" property="collegeNo" />
        <result column="college_name" property="collegeName" />
        <result column="city" property="city" />
        <result column="ranking" property="ranking" />
        <result column="person_count" property="personCount" />
        <result column="detail_info" property="detailInfo" />
        <result column="created_time" property="createdTime" />
        <result column="created_user" property="createdUser" />
        <result column="updated_time" property="updatedTime" />
        <result column="updated_user" property="updatedUser" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, college_no, college_name, city, ranking, person_count, detail_info, created_time, created_user,
        updated_time, updated_user
    </sql>

</mapper>
